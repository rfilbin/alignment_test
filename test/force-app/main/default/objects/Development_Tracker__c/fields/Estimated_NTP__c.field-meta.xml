<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_NTP__c</fullName>
    <description>Kickoff call completed date plus estimate NTP timeline</description>
    <externalId>false</externalId>
    <formula>Date(Year(Estimated_NTP_Timeline_days__c + Kickoff_Call_Completed__c),month(Estimated_NTP_Timeline_days__c + Kickoff_Call_Completed__c)+1,1)-1</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This is equal to kickoff call completed date plus estimated NTP timeline</inlineHelpText>
    <label>Estimated NTP</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
